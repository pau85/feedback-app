{
    "compilerOptions": {
      "target": "ES6",
      "module": "esnext",
      "lib": ["dom", "es2015", "es2017"],
      "jsx": "react",
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src"]
  }
  